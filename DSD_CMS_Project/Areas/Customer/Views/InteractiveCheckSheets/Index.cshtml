@model List<InteractiveChecksheets>

<div class="card box-shadow border-0 mt-4">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">Interactive Checksheets Details</h2>
            </div>
        </div>
    </div>

    <div class="card-body p-4">
        <div class="row pb-3">
            <div class="col-6">
                <h3 class="text-primary">
                    Interactive Checksheets list
                </h3>
            </div>

            <div class="col-6 text-end">
                <a asp-controller="InteractiveCheckSheets" asp-action="Upsert" class="btn btn-primary">
                    <i class="bi bi-plus-circle"></i>  New Interactive CheckSheets
                </a>
            </div>
        </div>

        <table id="inCSData" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Category</th>
                    <th>Question</th>
                    <th> </th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/interactiveCS.js"></script>
}